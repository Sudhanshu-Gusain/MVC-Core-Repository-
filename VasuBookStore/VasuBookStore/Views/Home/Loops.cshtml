
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Loops</title>
</head>
<body>
    @for(int i= 0; i<10;i++)
    {
        <h1>Hello @i</h1>
    }

    @{
        var list = new List<int>(){ 1, 2, 3, 4, 5, 6 };
    }
    @foreach (var item in list)
    {
        <h1>This is item : @item</h1>
    }
</body>
</html>
